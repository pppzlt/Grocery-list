{{!-- grocery cards that populate based on how many items are in the db --}}
<div class="card d-flex justify-content-center col-6 mt-3">

    <div class="card-body d-flex justify-content-between shadow">
        <div>
            <h5 class="card-title">{{item}}</h5>
            <span class = "d-flex flex-column">
                <span> 
                    quantity: {{quantity}}
                </span>
                <span>
                    frequency: {{frequency}}
                </span>
            </span>
            {{#if check}}{{else}}
                <span>List: {{list.list_name}}</span>
            {{/if}}
            <span>comment: {{comment}}</span>
        </div>
        <div class="d-flex flex-column justify-content-center">
            <a href="javascript:;" class="card-link">
                <h4 hidden>{{items_id}}</h4>
                <div class="material-symbols-outlined px-3" id="delete">
                    delete
                </div>
            </a>
        </div>
    </div>
</div>